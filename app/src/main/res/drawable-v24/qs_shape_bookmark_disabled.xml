<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/offStateColor" />
            <corners android:radius="24dp" />
        </shape>
    </item>
    <item
        android:width="14dp"
        android:height="14dp"
        android:drawable="@drawable/qs_shape_bookmark_arrow_disabled_bg"
        android:gravity="left|center"
        android:left="43dp" />
    <item
        android:width="14dp"
        android:height="14dp"
        android:drawable="@drawable/qs_shape_bookmark_arrow_fg"
        android:gravity="left|center"
        android:left="43dp" />
    <item android:gravity="left|center">
        <shape android:shape="rectangle">
            <solid android:color="@color/offStateColor" />
            <corners
                android:bottomLeftRadius="24dp"
                android:topLeftRadius="24dp" />
            <size
                android:width="50dp"
                android:height="60dp" />
        </shape>
    </item>
    <item android:gravity="left|center">
        <shape android:shape="rectangle">
            <solid android:color="@color/offStateColor3" />
            <corners
                android:bottomLeftRadius="24dp"
                android:topLeftRadius="24dp" />
            <size
                android:width="50dp"
                android:height="60dp" />
        </shape>
    </item>
</layer-list>