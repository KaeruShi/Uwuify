<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:systemui="http://schemas.android.com/apk/res-auto"
    android:gravity="bottom"
    android:orientation="horizontal"
    android:id="@*com.android.systemui:id/quick_status_bar_system_icons"
    android:clickable="true"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:layout_marginHorizontal="15dp"
    android:layout_width="fill_parent"
    android:layout_height="@*android:dimen/quick_qs_offset_height"
    android:paddingStart="15dp"
    android:paddingEnd="15dp">
    <FrameLayout
        android:gravity="bottom"
        android:layout_width="fill_parent"
        android:background="@*com.android.systemui:drawable/mm_head"
        android:layout_marginTop="0dip"
        android:layout_height="match_parent">
        <LinearLayout
            android:gravity="top|left"
            android:layout_gravity="top|left"
            android:layout_marginTop="0dp"
            android:orientation="vertical"
            android:paddingStart="0.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextClock
                android:textColor="#ffff"
                android:textSize="32sp"
                android:singleLine="true"
                android:textStyle="bold"
                android:textAllCaps="false"
                android:includeFontPadding="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:format12Hour="hh"
                android:format24Hour="HH"
                android:textAppearance="@*com.android.systemui:style/TextAppearance.QS.Status" />
            <TextClock
                android:textColor="#ffff"
                android:textSize="32sp"
                android:singleLine="true"
                android:textStyle="bold"
                android:textAllCaps="false"
                android:includeFontPadding="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:format12Hour="mm"
                android:format24Hour="MM"
                android:textAppearance="@*com.android.systemui:style/TextAppearance.QS.Status" />
            <TextClock
                android:textColor="#ffff"
                android:textSize="15sp"
                android:singleLine="true"
                android:textAllCaps="false"
                android:includeFontPadding="false"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:format12Hour="EEE, dd MMM"
                android:format24Hour="EEE, dd MMM"
                android:textAppearance="@*com.android.systemui:style/TextAppearance.QS.Status" />
        </LinearLayout>
        <LinearLayout
            android:paddingEnd="10.0dip"
            android:paddingTop="10dp"
            android:gravity="end|top"
            android:layout_gravity="end|top"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="end|top"
                android:layout_gravity="end|top"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <com.android.systemui.rr.statusbarweather.QsHeaderWeatherImage
                    android:gravity="center"
                    android:layout_gravity="end|center"
                    android:id="@*com.android.systemui:id/weather_image"
                    android:visibility="gone"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:paddingStart="2.0dip"
                    android:paddingEnd="2.0dip" />
                <com.android.systemui.rr.statusbarweather.QsHeaderWeather
                    android:textAppearance="@*com.android.systemui:style/TextAppearance.StatusBar.Clock"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:layout_gravity="end|center"
                    android:id="@*com.android.systemui:id/weather_temp"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="5.0dip"
                    systemui:showDark="false" />
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|top"
                android:gravity="end|top"
                android:textSize="16sp"
                android:textColor="#ffff"
                android:textStyle="bold"
                android:text="@*com.android.systemui:string/stream_system_enforced" />
        </LinearLayout>
        <FrameLayout
            android:gravity="start|center"
            android:layout_gravity="bottom"
            android:paddingStart="15.0dip"
            android:paddingEnd="15.0dip"
            android:layout_width="fill_parent"
            android:layout_marginBottom="20dip"
            android:layout_height="40dp">
            <LinearLayout
                android:gravity="start|center"
                android:layout_gravity="start|bottom"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="24dp">
                <ImageView
                    android:id="@*com.android.systemui:id/qsb_daily_data_usage_icon"
                    android:visibility="gone"
                    android:layout_width="16dp"
                    android:gravity="start|center"
                    android:tint="?android:attr/textColorPrimary"
                    android:layout_height="16dp"
                    android:src="@*com.android.systemui:drawable/ic_data_saver_off" />
                <com.android.systemui.statusbar.info.DataUsageView
                    android:textAppearance="@*com.android.systemui:style/TextAppearance.StatusBar.Clock"
                    android:textSize="14sp"
                    android:gravity="start|center"
                    android:id="@*com.android.systemui:id/qsb_data_sim_usage"
                    android:textColor="?android:attr/textColorPrimary"
                    android:paddingStart="5dip"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    systemui:showDark="false" />
            </LinearLayout>
            <LinearLayout
                android:gravity="end|bottom"
                android:layout_gravity="end|bottom"
                android:orientation="horizontal"
                android:paddingStart="0.0dip"
                android:layout_marginTop="0.0dip"
                android:layout_alignParentEnd="true"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent">
                <com.android.systemui.statusbar.phone.StatusIconContainer
                    android:id="@*com.android.systemui:id/statusIcons"
                    android:gravity="end|bottom"
                    android:layout_width="wrap_content"
                    android:layout_height="24dp"
                    android:layout_marginRight="3.0dip" />
                <com.android.systemui.BatteryMeterView
                    android:id="@*com.android.systemui:id/batteryRemainingIcon"
                    android:layout_width="wrap_content"
                    android:layout_height="24dp"
                    android:singleLine="true" />
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
    <LinearLayout
        android:gravity="end|center"
        android:orientation="horizontal"
        android:layout_width="0.0dip"
        android:layout_height="fill_parent"
        android:layout_weight="1.0">

        <com.android.systemui.rr.logo.LogoImageViewQuick
            android:id="@*com.android.systemui:id/statusbar_logo"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:scaleType="center"
            android:paddingStart="5dp" />
        <com.android.systemui.BatteryMeterView
            android:id="@*com.android.systemui:id/batteryIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center" />
        <com.android.systemui.statusbar.policy.Clock
            android:textAppearance="@*com.android.systemui:style/TextAppearance.StatusBar.Clock"
            android:gravity="start|center"
            android:id="@*com.android.systemui:id/clock"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:singleLine="true"
            android:paddingStart="@*com.android.systemui:dimen/status_bar_left_clock_starting_padding"
            android:paddingEnd="@*com.android.systemui:dimen/status_bar_left_clock_end_padding"
            systemui:showDark="false" />
    </LinearLayout>
    <android.widget.Space
        android:layout_gravity="end|"
        android:id="@*com.android.systemui:id/space"
        android:visibility="gone"
        android:layout_width="0.0dip"
        android:layout_height="0dp" />
    <LinearLayout
        android:gravity="end|center"
        android:orientation="horizontal"
        android:paddingEnd="10.0dip"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent">
        <LinearLayout
            android:gravity="end|center"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent">
            <include layout="@*com.android.systemui:layout/ongoing_privacy_chip" />
            <com.android.systemui.statusbar.policy.NetworkTraffic
                android:id="@*com.android.systemui:id/network_traffic"
                android:layout_width="0dp"
                android:layout_height="fill_parent" />
            <LinearLayout
                android:gravity="end|center"
                android:orientation="horizontal"
                android:id="@*com.android.systemui:id/system_info_layout"
                android:clipChildren="false"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:paddingStart="2.0dip"
                android:paddingEnd="2.0dip">
                <ImageView
                    android:id="@*com.android.systemui:id/system_info_icon"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="@*com.android.systemui:dimen/status_bar_icon_size" />
                <TextView
                    android:textAppearance="@*com.android.systemui:style/TextAppearance.StatusBar.Clock"
                    android:gravity="end|center"
                    android:id="@*com.android.systemui:id/system_info_text"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:singleLine="true" />
            </LinearLayout>
            <com.android.systemui.rr.logo.LogoImageViewQuickRight
                android:gravity="center_vertical"
                android:id="@*com.android.systemui:id/statusbar_logo_right"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:scaleType="center"
                android:paddingStart="2.0dip" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>